{
	"name": "ed/php-cs-fixer-config",
	"description": "Provides a configuration for php-cs-fixer",
	"license": "proprietary",
	"authors": [
		{
			"name": "Elan Ruusam√§e",
			"email": "glen@delfi.ee"
		}
	],
	"config": {
		"platform": {
			"php": "5.6.0"
		},
		"sort-packages": true
	},
	"require": {
		"php": "^5.6 || ^7.0"
	},
	"require-dev": {
		"composer/semver": "^1.0",
		"friendsofphp/php-cs-fixer": "^2.0",
		"phpunit/phpunit": "^4.8.36",
		"symfony/process": "^2.7 || ^3.0 || ^4.0"
	},
	"autoload": {
		"psr-4": {
			"ED\\CS\\Config\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ED\\CS\\Config\\Test\\": "tests/"
		}
	}
}
